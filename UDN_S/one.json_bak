        "\u95dc\u6ce8\u653f\u6cbb\u8cc7\u8a0a": 0.9937,
        "\u95dc\u6ce8\u5730\u65b9\u65b0\u805e": 0.893,
        "\u95dc\u6ce8\u6642\u5c1a\u6f6e\u6d41": 0.8475
